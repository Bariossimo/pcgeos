#if _PEN_BASED
FolderInfo = NoQTText {
#else
FolderInfo = GenText {
#endif
	genAttributes = readOnly;
	textDisplayAttributes = DESK_SINGLE_LINE_DISPLAY_ATTRIBUTES,
					allowTextOffEnd, singleLineText;
	hints = {
		HINT_EXPAND_WIDTH_TO_FIT_PARENT,
		HINT_INITIAL_SIZE {
			SpecWidth <SST_PIXELS, 10>,
			SpecHeight <0, 0>
		}
	}
}
end FolderWindowTemplate;


